    "#cifrado modular\n",
    "import math\n",
    "\n",
    "def encriptar(x,e,n):\n",
    "    cipher=''\n",
    "    for i in range(len(x)):\n",
    "        block=str(hex(int(math.pow(int(x[i]),e)%n)))[2:].zfill(2)\n",
    "        print(int(math.pow(int(x[i]),e)%n),block)\n",
    "        cipher +=block\n",
    "    return(cipher)\n",
    "def desencriptar(x,d,n):\n",
    "    decif=''\n",
    "    for i in range(int(len(x)/2)):\n",
    "        print(int(x[i*2:i*2+2],16),str(int(math.pow(int(x[i*2:i*2+2],16),d)%n)))\n",
    "        decif+=str(int(math.pow(int(x[i*2:i*2+2],16),d)%n))\n",
    "        # cipher +=str(int(math.pow(int(x[i]),d)%n))\n",
    "    return(decif)\n",
    "encriptar('308106325',e,n)"
